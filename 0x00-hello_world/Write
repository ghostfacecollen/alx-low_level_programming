#include <stdio.h>

int main() {
    printf("           ;\\\n");
    printf("         |' \\\\            __\n");
    printf("         ;   \\\\        .-\"  `-.\n");
    printf("        /     ;       /        \\\n");
    printf("       /      |      ;          \\\n");
    printf("      /       ;   .-\"            ;\n");
    printf("     /        |.-'                \\\n");
    printf("    ;          \\                  |\n");
    printf("   /             '.               |\n");
    printf("  /                \\              |\n");
    printf(" ;                  '.            ;\n");
    printf("|                     \\           |\n");
    printf("|     .                ;          |\n");
    printf("|    /                 |          |\n");
    printf("|   ;.'.               ;          |\n");
    printf("|   |   \\              |          |\n");
    printf("|   |    \\            .)          |\n");
    printf("|   \\    |            \\           |\n");
    printf("|    |   \\            |           |\n");
    printf("|    |    ;           \\           |\n");
    printf("|___:   //            |           |\n");
    printf("    |  .'/             |          |\n");
    printf("    '--'               |          |\n");
    printf("                        \\        /\n");
    printf("                         '-..__.-'  \n");
    
    return 0;
}

